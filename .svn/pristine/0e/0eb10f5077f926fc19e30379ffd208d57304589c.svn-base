package com.sjy.entity;

import java.util.Date;

public class StockCode {
	private Integer stockCodeId;
	private String thscode;
	private String securityName;
	private String market;
	private Date date;
	private Integer isHot;
	private Integer isIndex;
	private Double bonedWarehouse;
	
	//关联数据查询
	private Double lastestPrice; //最新价
	private String changeRatio;// 涨跌幅

	public String getMarket() {
		return market;
	}

	public void setMarket(String market) {
		this.market = market;
	}

	public Integer getStockCodeId() {
		return stockCodeId;
	}
	public String getThscode() {
		return thscode;
	}
	public String getSecurityName() {
		return securityName;
	}
	public Date getDate() {
		return date;
	}
	public Integer getIsHot() {
		return isHot;
	}
	public Integer getIsIndex() {
		return isIndex;
	}
	public Double getBonedWarehouse() {
		return bonedWarehouse;
	}
	public void setStockCodeId(Integer stockCodeId) {
		this.stockCodeId = stockCodeId;
	}
	public void setThscode(String thscode) {
		this.thscode = thscode;
	}
	public void setSecurityName(String securityName) {
		this.securityName = securityName;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	public void setIsHot(Integer isHot) {
		this.isHot = isHot;
	}
	public void setIsIndex(Integer isIndex) {
		this.isIndex = isIndex;
	}
	public void setBonedWarehouse(Double bonedWarehouse) {
		this.bonedWarehouse = bonedWarehouse;
	}
	public Double getLastestPrice() {
		return lastestPrice;
	}
	public void setLastestPrice(Double lastestPrice) {
		this.lastestPrice = lastestPrice;
	}
	public String getChangeRatio() {
		return changeRatio;
	}
	public void setChangeRatio(String changeRatio) {
		this.changeRatio = changeRatio;
	}

	@Override
	public String toString() {
		return "StockCode{" +
				"stockCodeId=" + stockCodeId +
				", thscode='" + thscode + '\'' +
				", securityName='" + securityName + '\'' +
				", market='" + market + '\'' +
				", date=" + date +
				", isHot=" + isHot +
				", isIndex=" + isIndex +
				", bonedWarehouse=" + bonedWarehouse +
				", lastestPrice=" + lastestPrice +
				", changeRatio='" + changeRatio + '\'' +
				'}';
	}
}
