function pr_style(s) {
    var re_pr = 0;
    var re_str = "";
    if (s > 10000 && s < 10000000) {
        re_pr = s / 10000;
        re_str = '万';
    } else if (s > 10000000 && s < 100000000) {
        re_pr = s / 10000000;
        re_str = '千万';
    } else if (s >= 100000000) {
        re_pr = s / 100000000;
        re_str = '亿';
    } else if (s < 10000) {
        re_pr = s;
    }
    if (s < 10000) {
        re_pr = re_pr;
    } else {
        re_pr = re_pr.toFixed(2);
    }
    return (re_pr + re_str);
}
